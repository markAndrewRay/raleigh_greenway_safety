{
    "name": "Raleigh Greenway Safety Analysis",
    "image": "mcr.microsoft.com/devcontainers/python:3.11",
    "features": {
        "ghcr.io/devcontainers/features/git-lfs:1": {},
        "ghcr.io/eitsupi/devcontainer-features/duckdb-cli:1": {}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "innoverio.vscode-dbt-power-user",
                "ms-python.python",
                "ms-toolsai.jupyter"
            ]
        }
    },
    "postCreateCommand": "pip3 install -r requirements.txt && git lfs install && git lfs pull"
}